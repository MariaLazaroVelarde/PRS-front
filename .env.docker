# Configuración para Docker
NODE_ENV=production

# Configuración de la aplicación
APP_NAME=Sistema JASS
APP_VERSION=1.0.0

# Puerto de la aplicación (usado internamente)
APP_PORT=80

# URL base de la API (ajusta según tu backend)
